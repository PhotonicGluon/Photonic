{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true
    },
    "editor.formatOnSave": true,
    "cSpell.words": ["pyproject", "stylesheet", "twgl", "uvicorn"],
    "files.insertFinalNewline": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
}
